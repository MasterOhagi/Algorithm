// https://programmers.co.kr/learn/courses/30/lessons/42576?language=javascript

function marathon(participant, completion) {
    var array = [];
    var newarray = [];
    var answer = '';
    var newarrayToString = ''; 
    
    for(var i in participant) {
        for(var j in completion) {
            if(completion[j] === participant[i]) {
            array.push(completion[j]);
            } else {
            answer = participant[j];
            }
        }
    }
    
    function removeDuplicates(data) {
        return [...new Set(data)];
    }
    
    newarray.push(removeDuplicates(array));
    newarrayToString = newarray.toString();
    console.log('Completion : ' + newarrayToString);
    
    return answer;
}

function solution(participant, completion) {
    var answer = '';

    participant.sort()
    completion.sort()
    
    for(let i in participant){
        if(participant[i]!==completion[i]){
        answer = participant[i];
        break;
        }
    }

    return answer;
}

console.log(solution(["nikola", "nikola", "kiki", "kiki", "eden", "eden"], ["kiki", "eden"]));
